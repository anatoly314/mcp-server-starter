{
  "name": "mcp-server-starter",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "start": "tsx -r dotenv/config src/server.ts",
    "build": "tsc",
    "dev": "tsx watch -r dotenv/config src/server.ts",
    "dev:stdio": "DOTENV_CONFIG_PATH=.env.stdio tsx watch -r dotenv/config src/server.ts",
    "dev:http": "DOTENV_CONFIG_PATH=.env.http.clerk tsx watch -r dotenv/config src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "inspector:http": "./node_modules/.bin/mcp-inspector --config mcp-inspector-config.json --server http-server",
    "inspector:stdio": "./node_modules/.bin/mcp-inspector --no-open --config mcp-inspector-config.json --server stdio-server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-prometheus": "^0.203.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "google-auth-library": "^10.3.0",
    "ip-range-check": "^0.2.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.9.5",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.16.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.3",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3"
  }
}
